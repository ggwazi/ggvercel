{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Sandbox Execute Request",
  "type": "object",
  "required": ["code"],
  "properties": {
    "code": {
      "type": "string",
      "description": "Code to execute in the sandbox"
    },
    "runtime": {
      "type": "string",
      "enum": ["node22", "node24", "python3.13"],
      "default": "node22"
    },
    "timeout": {
      "type": "integer",
      "minimum": 1000,
      "maximum": 300000,
      "default": 30000,
      "description": "Timeout in milliseconds"
    }
  }
}
